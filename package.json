{
  "name": "onesait-banking",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all --parallel localserver ngserve",
    "localserver": "node server.js",
    "ngserve": "ng serve --open --port 4200",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server": "node server.js",
    "generate-libs": "node ./hooks/generate-libs.js",
    "packagr-libs": "node ./hooks/packagr-libs.js",
    "publish-libs": "node ./hooks/publish-libs.js",
    "checkout-raml": "run-func ./hooks/raml.js checkout",
    "update-raml": "run-func ./hooks/raml.js update",
    "generate-war": "ng build --prod --configuration uat --base-href /onesaitbusiness/ &&  run-func ./hooks/generate-war.js checkout"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
